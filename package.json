{
  "name": "npm-run-tests",
  "version": "0.1.0",
  "description": "Run tests of the given globally or locally installed npm package, e.g. `npm test <packageName>`",
  "scripts": {
    "lint": "jshint index.js && jscs index.js --reporter inline",
    "test": "node test.js",
    "test-cov": "istanbul cover test.js",
    "test-travis": "istanbul cover test.js --report lcovonly"
  },
  "author": {
    "name": "Charlike Mike Reagent",
    "email": "mameto_100@mail.bg",
    "url": "https://github.com/tunnckoCore"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tunnckoCore/npm-run-tests.git"
  },
  "keywords": [
    "global",
    "local",
    "npm",
    "package",
    "pkg",
    "run",
    "testing",
    "tests"
  ],
  "license": {
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/npm-run-tests/blob/master/license.md"
  },
  "dependencies": {
    "delete": "~0.1.5",
    "get-installed-path": "~1.0.0",
    "spawn-commands": "~0.2.0"
  },
  "devDependencies": {
    "assertit": "^0.1.0",
    "istanbul-harmony": "~0.3.1"
  },
  "bin": {
    "npm-test": "cli.js",
    "npm-run-tests": "cli.js"
  }
}